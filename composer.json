{
    "name": "pleodigital/circle-ci-webhook",
    "description": "Plugin adding CP button triggering CircleCI.",
    "type": "craft-plugin",
    "version": "1.0.5",
    "keywords": [
        "craft",
        "cms",
        "craftcms",
        "craft-plugin",
        "circleci webhook"
    ],
    "support": {
        "docs": "https://github.com/pleodigital/circle-ci-webhook",
        "issues": "https://github.com/pleodigital/circle-ci-webhook"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Pleo Digtial",
            "homepage": "https://pleodigital.com/"
        }
    ],
    "require": {
        "craftcms/cms": "^3.0.0-RC1|^4.0",
        "php": "^7.2.5|^8.0"
    },
    "autoload": {
        "psr-4": {
          "pleodigital\\circleciwebhook\\": "src/"
        }
    },
    "extra": {
        "name": "CircleCi Webhook",
        "handle": "circle-ci-webhook",
        "hasCpSettings": true,
        "hasCpSection": false,
        "changelogUrl": "???",
